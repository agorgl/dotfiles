{
  "clock": {
    "format": "<span size=\"9pt\" rise=\"-2.3pt\"></span> {:%H:%M}",
    "format-alt": "<span size=\"9pt\" rise=\"-2.3pt\"></span> {:%a, %d %b %H:%M}",
    "tooltip-format": "<tt><big>{calendar}</big></tt>",
    "calendar": {
      "format": {
        "today": "<b><u>{}</u></b>"
      }
    }
  }
}
