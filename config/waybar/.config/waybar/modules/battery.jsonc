//
// waybar/modules/battery.jsonc
//

{
  "battery": {
    "interval": 1,
    "format": "<span size=\"9pt\" rise=\"-2.3pt\">{icon}</span> {capacity}%",
    "format-alt": "<span size=\"9pt\" rise=\"-2.3pt\">{icon}</span> {capacity}%/{power}W",
    "format-time": "{H}:{M}",
    "format-icons": {
      "charging": "",
      "discharging": ""
    }
  }
}
